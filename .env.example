# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# AI Engine
GEMINI_API_KEY=your_gemini_api_key

# Application
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000
PIXEL_URL=http://localhost:3002

# Session & Security
SESSION_SECRET=your_session_secret
JWT_SECRET=your_jwt_secret
COOKIE_DOMAIN=localhost

# OAuth App Registration (for the platform itself, not user credentials)
# Register your app with each platform to get these credentials
# Users will connect their accounts through OAuth flows in the UI
GOOGLE_OAUTH_CLIENT_ID=your_app_oauth_client_id
GOOGLE_OAUTH_CLIENT_SECRET=your_app_oauth_client_secret
META_OAUTH_APP_ID=your_app_oauth_app_id
META_OAUTH_APP_SECRET=your_app_oauth_app_secret
STRIPE_OAUTH_CLIENT_ID=your_app_oauth_client_id
PAYPAL_OAUTH_CLIENT_ID=your_app_oauth_client_id
HUBSPOT_OAUTH_CLIENT_ID=your_app_oauth_client_id
MAILCHIMP_OAUTH_CLIENT_ID=your_app_oauth_client_id

# OAuth Redirect URIs
OAUTH_REDIRECT_URI=http://localhost:3001/api/oauth/callback
