# Sanitized example - replace with your own values
VITE_SUPABASE_URL="https://your-supabase-project.supabase.co/"
VITE_SUPABASE_ANON_KEY="your_supabase_anon_key"

# Minimal config for development
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000
PIXEL_URL=http://localhost:3002

# Supabase
SUPABASE_URL="https://your-supabase-project.supabase.co"
SUPABASE_ANON_KEY="your_supabase_anon_key"
SUPABASE_SERVICE_ROLE_KEY="your_supabase_service_role_key"

# Session & Security
SESSION_SECRET=dev_secret_key_change_in_production
JWT_SECRET=dev_jwt_secret_change_in_production
COOKIE_DOMAIN=localhost

# Token Encryption (for storing OAuth tokens securely)
TOKEN_ENCRYPTION_KEY=dev_encryption_key_32_bytes_long_change_in_production

# OAuth App Credentials (Register your app with each platform to get these)
GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id
GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret

# Meta (Facebook/Instagram)
# SanitUTH_APP_ID=your_meta_oauth_app_id
META_OAUTH_APP_SECRET=your_meta_oauth_app_secret

# Stripe
STRIPE_OAUTH_CLIENT_ID=your_stripe_client_id
STRIPE_SECRET_KEY=your_stripe_secret_key

# PayPal
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
